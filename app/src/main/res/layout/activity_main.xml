<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="583dp"
        android:layout_height="1067dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hbg" />



    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="102dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.45"
        app:srcCompat="@drawable/head" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="wrap_content"
        android:layout_height="412dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/imageView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/learn"
                android:layout_width="276dp"
                android:layout_height="163dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/secure" />

            <ImageView
                android:id="@+id/fraud1"
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/fraud" />

            <ImageView
                android:id="@+id/qna"
                android:layout_width="276dp"
                android:layout_height="163dp"
                android:layout_weight="1"
                android:onClick="tap"
                android:tag="quesandans"
                app:srcCompat="@drawable/qna" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/fraud0"
                android:layout_width="276dp"
                android:layout_height="163dp"
                android:layout_weight="1" />

            <ImageView
                android:id="@+id/fraud2"
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/fraud" />

            <ImageView
                android:id="@+id/fraud3"
                android:layout_width="276dp"
                android:layout_height="163dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/vnp"
                android:layout_width="276dp"
                android:layout_height="163dp"
                android:onClick="tap"
                android:tag="vnp"
                android:layout_weight="1"
                app:srcCompat="@drawable/vnp" />

            <ImageView
                android:id="@+id/fraud440"
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/fraud" />

            <ImageView
                android:id="@+id/gnc"
                android:layout_width="273dp"
                android:layout_height="163dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/gnc" />
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/imageViewpp"
        android:layout_width="144dp"
        android:layout_height="133dp"
        android:onClick="policeno"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.43"
        app:srcCompat="@drawable/police" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="wrap_content"
        android:layout_height="131dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        app:srcCompat="@drawable/bottom" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="174dp"
        android:layout_height="21dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/fraud" />

</androidx.constraintlayout.widget.ConstraintLayout>